<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.text.Font?>

<ButtonBar buttonMinWidth="100.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.view.StudentBBViewController">
  <buttons>
    <Button mnemonicParsing="false" onAction="#acept" onMouseEntered="#handCursor" onMouseExited="#defaultCursor" text="Aceptar">
         <font>
            <Font size="18.0" />
         </font></Button>
      <HBox prefWidth="100.0" />
      <Button mnemonicParsing="false" onAction="#cancel" onMouseEntered="#handCursor" onMouseExited="#defaultCursor" text="Cancelar">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
  </buttons>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="40.0" right="50.0" />
   </padding>
</ButtonBar>
